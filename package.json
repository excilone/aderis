{
	"name": "@excilone/aderis",
	"description": "Adds new methods and properties to the Eris library",
	"version": "0.1.2",
	"main": "dist/index.js",
	"types": "./index.d.ts",
	"files": [
		"dist/**/*",
		"*.d.ts"
	],
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/config.js",
			"types": "./index.d.ts"
		},
		"./config": {
			"require": "./dist/config.js",
			"import": "./dist/config.js",
			"types": "./index.d.ts"
		},
		"./config.js": {
			"require": "./dist/config.js",
			"import": "./dist/config.js",
			"types": "./index.d.ts"
		}
	},
	"license": "Apache-2.0",
	"dependencies": {
		"@excilone/collection": "^0.0.2",
		"eris": "^0.17.2"
	},
	"devDependencies": {
		"@types/node": "^18.15.11",
		"@types/ws": "^8.5.3",
		"@typescript-eslint/eslint-plugin": "^5.14.0",
		"@typescript-eslint/parser": "^5.14.0",
		"eslint": "^8.11.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.25.4",
		"prettier": "^2.5.1",
		"typescript": "^4.6.2"
	},
	"scripts": {
		"build": "tsc -p .",
		"test": "node index.test.js",
		"test-esm": "node index.test.mjs",
		"lint": "eslint .",
		"lint-fix": "eslint . --fix",
		"format": "prettier --write .",
		"prepublishOnly": "yarn format && yarn lint && yarn build"
	},
	"author": {
		"name": "cr1xto"
	},
	"packageManager": "yarn@1.22.19",
	"keywords": [
		"discord",
		"excilone",
		"eris",
		"aderis"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/excilone/aderis.git"
	},
	"publishConfig": {
		"access": "public"
	}
}
