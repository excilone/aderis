{
	"name": "@excilone/aderis",
	"description": "Adds new methods and properties to the Eris library",
	"version": "0.0.1",
	"main": "dist/index.js",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"types": "./index.d.ts",
			"default": "./dist/index.js"
		},
		"./config": {
			"require": "./dist/config.js",
			"types": "./src/typings/config.d.ts",
			"default": "./dist/config.js"
		}
	},
	"license": "Apache-2.0",
	"dependencies": {
		"eris": "^0.16.1"
	},
	"devDependencies": {
		"@types/node": "^17.0.23",
		"@types/ws": "^8.5.3",
		"@typescript-eslint/eslint-plugin": "^5.14.0",
		"@typescript-eslint/parser": "^5.14.0",
		"eslint": "^8.11.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.25.4",
		"prettier": "^2.5.1",
		"typescript": "^4.6.2"
	},
	"scripts": {
		"build": "tsc -p .",
		"test": "node index.test.js",
		"lint": "eslint .",
		"lint-fix": "eslint . --fix",
		"format": "prettier --write .",
		"prepublishOnly": "yarn format && yarn lint && yarn build"
	},
	"author": {
		"name": "cr1xto"
	},
	"packageManager": "yarn@1.22.15",
	"keywords": [
		"discord",
		"excilone",
		"eris",
		"aderis"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/excilone/aderis.git"
	},
	"publishConfig": {
		"access": "public"
	}
}
